#include <iostream>
#include <queue>
#include <vector>
using namespace std;

int main() {
    vector<vector<int>> g = {{1,2},{0,3},{0,3},{1,2}};
    vector<bool> vis(4,false);
    queue<int> q;
    q.push(0); vis[0]=true;
    while(!q.empty()){
        int u=q.front();q.pop();
        cout<<u<<" ";
        for(int v:g[u]) if(!vis[v]){ vis[v]=true; q.push(v); }
    }
}
