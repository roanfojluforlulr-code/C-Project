#include <iostream>
#include <vector>
using namespace std;

void dfs(int u, vector<vector<int>>& g, vector<bool>& vis) {
    vis[u] = true;
    cout << u << " ";
    for (int v : g[u]) if (!vis[v]) dfs(v,g,vis);
}

int main() {
    vector<vector<int>> g = {{1,2},{0,3},{0,3},{1,2}};
    vector<bool> vis(4,false);
    dfs(0,g,vis);
}
